<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fono"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/f3"
    tools:context=".RegistroN">

    <EditText
        android:id="@+id/nombreape"
        android:layout_width="wrap_content"
        android:layout_height="82dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="85dp"
        android:layout_marginTop="245dp"
        android:layout_marginEnd="103dp"
        android:layout_marginBottom="404dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nombre y Apellido"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/edad"
        android:layout_width="wrap_content"
        android:layout_height="78dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="84dp"
        android:layout_marginTop="334dp"
        android:layout_marginEnd="103dp"
        android:layout_marginBottom="319dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Edad"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/tele"
        android:layout_width="wrap_content"
        android:layout_height="68dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="84dp"
        android:layout_marginTop="417dp"
        android:layout_marginEnd="104dp"
        android:layout_marginBottom="246dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Teléfono"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/direccion"
        android:layout_width="wrap_content"
        android:layout_height="74dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="82dp"
        android:layout_marginTop="490dp"
        android:layout_marginEnd="103dp"
        android:layout_marginBottom="167dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Dirección"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/provin"
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="82dp"
        android:layout_marginTop="567dp"
        android:layout_marginEnd="102dp"
        android:layout_marginBottom="94dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Provincia"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/email"
        android:layout_width="wrap_content"
        android:layout_height="68dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="86dp"
        android:layout_marginTop="96dp"
        android:layout_marginEnd="102dp"
        android:layout_marginBottom="567dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="Email"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/pss"
        android:layout_width="wrap_content"
        android:layout_height="71dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="86dp"
        android:layout_marginTop="167dp"
        android:layout_marginEnd="101dp"
        android:layout_marginBottom="493dp"
        android:ems="10"
        android:inputType="textPassword"
        android:text="Password"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="18sp" />

    <Button
        android:id="@+id/btnGuardar"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="230dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="668dp"
        android:background="@color/colorPrimary"
        android:text="Guardar"
        android:textColor="@android:color/background_light"
        android:textSize="18sp" />

    <Button
        android:id="@+id/volver"
        android:layout_width="wrap_content"
        android:layout_height="43dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="36dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="222dp"
        android:layout_marginBottom="668dp"
        android:background="@color/colorPrimary"
        android:text="Volver"
        android:textColor="@android:color/background_light"
        android:textSize="18sp" />

    <CheckBox
        android:id="@+id/cbMedico"
        android:layout_width="170dp"
        android:layout_height="48dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="86dp"
        android:layout_marginTop="646dp"
        android:layout_marginEnd="155dp"
        android:layout_marginBottom="37dp"
        android:background="?android:attr/colorSecondary"
        android:text="Médico"
        android:textColor="@android:color/background_light"
        android:textColorLink="@android:color/holo_orange_light"
        android:textCursorDrawable="@android:color/holo_orange_light"
        android:textSize="18sp" />

</RelativeLayout>